    "keywords": [
        "yii2", "yii", "extension", "PHP",
        "asset", "bundle",
        "Start-Bootstrap", "sb-admin-2",
        "backend", "admin", "ui", "user-interface", "theme"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Plowman",
            "email": "p2opensource@gmail.com",
            "homepage": "https://github.com/p2made"
        }
    ],
    "repositories":
